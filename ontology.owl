<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:foaf="http://xmlns.com/foaf/0.1/"
         xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#"
         xmlns:hist="http://example.org/ontology/historical-podcast#">

    <!-- Ontology Header -->
    <owl:Ontology rdf:about="http://example.org/ontology/historical-podcast">
        <dc:title>Historical Podcast Ontology</dc:title>
        <dc:description>An ontology for linking podcast episodes to historical events, people, and locations.</dc:description>
        <dc:creator>Your Name</dc:creator>
        <dc:date>2024-12-05</dc:date>
    </owl:Ontology>

    <!-- Classes -->
    <owl:Class rdf:about="hist:Podcast"/>
    <owl:Class rdf:about="hist:PodcastSeries">
        <rdfs:subClassOf rdf:resource="hist:Podcast"/>
    </owl:Class>
    <owl:Class rdf:about="hist:PodcastEpisode">
        <rdfs:subClassOf rdf:resource="hist:Podcast"/>
    </owl:Class>

    <owl:Class rdf:about="hist:Person"/>
    <owl:Class rdf:about="hist:HistoricalFigure">
        <rdfs:subClassOf rdf:resource="hist:Person"/>
    </owl:Class>
    <owl:Class rdf:about="hist:Event"/>
    <owl:Class rdf:about="hist:HistoricalEvent">
        <rdfs:subClassOf rdf:resource="hist:Event"/>
    </owl:Class>
    <owl:Class rdf:about="hist:Place"/>
    <owl:Class rdf:about="hist:Artifact"/>

    <!-- Object Properties -->
    <owl:ObjectProperty rdf:about="hist:hasTopic">
        <rdfs:domain rdf:resource="hist:PodcastEpisode"/>
        <rdfs:range rdf:resource="hist:Event"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="hist:mentions">
        <rdfs:domain rdf:resource="hist:PodcastEpisode"/>
        <rdfs:range rdf:resource="hist:Person"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="hist:associatedWith">
        <rdfs:domain rdf:resource="hist:Person"/>
        <rdfs:range rdf:resource="hist:Event"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="hist:occurredAt">
        <rdfs:domain rdf:resource="hist:Event"/>
        <rdfs:range rdf:resource="hist:Place"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="hist:createdBy">
        <rdfs:domain rdf:resource="hist:Artifact"/>
        <rdfs:range rdf:resource="hist:Person"/>
    </owl:ObjectProperty>

    <!-- Data Properties -->
    <owl:DatatypeProperty rdf:about="hist:title">
        <rdfs:domain rdf:resource="hist:Podcast"/>
        <rdfs:range rdf:resource="xsd:string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="hist:releaseDate">
        <rdfs:domain rdf:resource="hist:PodcastEpisode"/>
        <rdfs:range rdf:resource="xsd:date"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="hist:startDate">
        <rdfs:domain rdf:resource="hist:Event"/>
        <rdfs:range rdf:resource="xsd:date"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="hist:endDate">
        <rdfs:domain rdf:resource="hist:Event"/>
        <rdfs:range rdf:resource="xsd:date"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="hist:description">
        <rdfs:domain rdf:resource="hist:Podcast"/>
        <rdfs:range rdf:resource="xsd:string"/>
    </owl:DatatypeProperty>

    <!-- Example Individuals -->
    <hist:PodcastEpisode rdf:about="http://example.org/podcast/episode1">
        <hist:title>World War II: The Road to War</hist:title>
        <hist:releaseDate rdf:datatype="xsd:date">2024-01-01</hist:releaseDate>
        <hist:hasTopic rdf:resource="http://example.org/event/wwii"/>
        <hist:mentions rdf:resource="http://example.org/person/winston_churchill"/>
    </hist:PodcastEpisode>

    <hist:HistoricalFigure rdf:about="http://example.org/person/winston_churchill">
        <foaf:name>Winston Churchill</foaf:name>
        <hist:associatedWith rdf:resource="http://example.org/event/wwii"/>
    </hist:HistoricalFigure>

    <hist:HistoricalEvent rdf:about="http://example.org/event/wwii">
        <hist:title>World War II</hist:title>
        <hist:startDate rdf:datatype="xsd:date">1939-09-01</hist:startDate>
        <hist:endDate rdf:datatype="xsd:date">1945-09-02</hist:endDate>
        <hist:occurredAt rdf:resource="http://example.org/place/europe"/>
    </hist:HistoricalEvent>

    <hist:Place rdf:about="http://example.org/place/europe">
        <geo:lat>50.1109</geo:lat>
        <geo:long>8.6821</geo:long>
        <foaf:name>Europe</foaf:name>
    </hist:Place>
</rdf:RDF>
